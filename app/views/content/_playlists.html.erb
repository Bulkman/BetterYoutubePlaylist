<div class="col-md-3">
  <div class="row">
    <div class="col-md-12">
      <div class="list-group">
        <a href="#" class="list-group-item active">TITLE LIST</a>
        <% get_playlists.each do |playlists_youtube| %>
            <%= link_to( set_playlist_id_path(:playlist_id => playlists_youtube["id"]), html_options = {:class => "list-group-item list-group-item-action"} ) do playlists_youtube["snippet"]["localized"]["title"] end %>
        <%end %>
      </div>
    </div>
  </div>
</div>